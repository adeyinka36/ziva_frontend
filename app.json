{
  "expo": {
    "name": "ziva_react_native",
    "slug": "ziva_react_native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "extra": {
      "devHost": "192.168.1.159",
      "reverAppId": 222942,
      "reverbAppKey": "cygxehin2xsdsgvycdrm",
      "reverbAppSecret": "fbwxhrkib40xqrssy10y",
      "reverbHost": "localhost",
      "reverbPort": 8080,
      "reverbScheme": "http",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6e27c21c-41b3-40f9-9bfd-1e5dce873c02"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.expo.ziva"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ziva_logo.png",
        "backgroundColor": "#ffc75f"
      },
      "package": "dev.expo.ziva",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/ziva_logo.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffc75f"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Select and upload images.",
          "camerapermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "yinkax86"
  }
}
